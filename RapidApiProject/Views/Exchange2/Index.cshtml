@model List<RapidApiProject.Models.ExchangeViewModel.Exchange_Rates>
@{
    Layout = null;
}
<link href="~/lib/bootstrap/dist/css/bootstrap.min.css" rel="stylesheet" />
<div class="container">
    <br />
    <h3>1 TL'nin Döviz Karşılığı</h3>
    <br /><br />
    <table class="table table-bordered">
        <tr>
            <td>Döviz Adı</td>
            <td>Döviz Tutarı</td>
        </tr>

        @foreach(var item in Model)
        {
            <tr>
                <td>@item.currency</td>
                <td>@item.exchange_rate_buy</td>
            </tr>
        }
    </table>
</div>